package pentagon_space;
public class Palindrome 
{
	public static void main(String[] args)
	{
		int num=12321;
		int temp=num;
		int res=0;
		while(num!=0)
		{
			int rem=num%10;
			res=res*10+rem;
			num=num/10;
		}
		if(res==temp)
		{
			System.out.println("Palindrome Number");
		}
		else
		{
			System.out.println("Not a Palindrome Number");
		}
	}
}
