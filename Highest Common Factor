package pentagon_space;
public class HCF 
{
	public static int find_hcf(int a, int b)
	{
		int hcf=1;
		int s=(a<b)?a:b;
		for(int i=1;i<=s;i++)
		{
			if(a%i==0 && b%i==0)
			{
				hcf=i;
			}
		}
		return hcf;
	}
	public static void main(String[] args)
	{
		int a=15,b=20;
		int res=find_hcf(a,b);
		System.out.println(res);
	}
}
