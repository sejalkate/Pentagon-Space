package pentagon_space;
import java.util.Scanner;
public class StringReverse 
{
	public static void main(String[] args)
	{
		String s="Computer";
		System.out.println("String : "+s);
		String res="";
		for(int i=s.length()-1;i>=0;i--)
		{
			res=res+s.charAt(i);
		}
		System.out.println("Reversed String : "+res);
	}
}
