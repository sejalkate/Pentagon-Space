package pentagon_space;
public class ReverseMethod 
{
	public static int findReverse(int num)
	{
		int res=0;
		while(num!=0)
		{
			int rem=num%10;
			res=(res*10)+rem;
			num=num/10;
		}
		return res;
	}
	public static void main(String[] args) 
	{
		int num=7654;
		int rev=findReverse(num);
		System.out.println(rev);
	}
}
